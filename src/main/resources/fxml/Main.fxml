<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE">
        <tabs>

          <Tab fx:id="dodajPomiar" text="Dodaj pomiar">
               <content>
                  <fx:include source="DodajPomiar.fxml" />
               </content></Tab>

          <Tab fx:id="rozpoznajOsobe" text="Rozpoznaj osobę">
              <content>
                  <fx:include source="RozpoznajOsobe.fxml" />
              </content></Tab>

            <Tab fx:id="badanieJakosciSystemu" text="Badanie jakości systemu">
                <content>
                    <fx:include source="BadanieJakosciSystemu.fxml" />
                </content></Tab>

        </tabs>
      </TabPane>
   </children>
</Pane>
